<div class="row">
  <div class="jumbotron">
    <div class="row">
      <div class="col-md-8"><h1>See your favorite Paintings unpixelated</h1></div>
      <div class="col-md-4">
    <%= image_tag('flaming_june.jpg', class: 'img-responsive img-rounded jumbo-image') %>
    <%= link_to "See our Paintings", paintings_path, class: 'btn btn-primary btn-block' %>
      </div>
  </div>
</div>
<div class="row featured">
	<div class="panel-default col-md-4">
		<h2>Museum Partners</h2>
		<%= link_to content_tag(:span, nil, class: "glyphicon glyphicon-home"), museums_path %>
	</div>
	<div class="panel-default col-md-4">
		<h2>Artist Partners</h2>
		<%= link_to content_tag(:span, nil, class: "glyphicon glyphicon-user"), artists_path %>
	</div>
	<div class="panel-default col-md-4">
		<h2>Paintings</h2>
	<%= link_to content_tag(:span, nil, class: "glyphicon glyphicon-picture"), paintings_path %>

	</div>
</div>